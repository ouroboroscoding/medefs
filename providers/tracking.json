{
	"__mysql__": {
		"auto_primary": true,
		"create": ["_created","_updated","memo_id","action","action_sesh",
					"action_ts","resolution","resolution_sesh","resolution_ts",
					"crm_type", "crm_id"],
		"db": "mems",
		"host": "primary",
		"indexes": {
			"user_crm": ["memo_id", "crm_type", "crm_id"],
			"sesh": null
		},
		"table": "providers_tracking",
		"charset": "utf8mb4",
		"collate": "utf8mb4_0900_ai_ci"
	},

	"__name__": "Providers_Tracking",

	"_id": {
		"__type__": "uint",
		"__optional__": true
	},

	"_created": {
		"__type__": "timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "NOT NULL default CURRENT_TIMESTAMP"
		}
	},

	"_updated": {
		"__type__": "timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP"
		}
	},

	"memo_id": {
		"__type__": "int"
	},

	"action": {
		"__type__": "string",
		"__options__": ["signin", "viewed"]
	},

	"action_sesh": {
		"__type__": "uuid"
	},

	"action_ts": {
		"__type__": "timestamp"
	},

	"resolution": {
		"__type__": "string",
		"__options__": ["signout", "new_signin", "approved", "declined", "transferred", "x"],
		"__optional__": true
	},

	"resolution_sesh": {
		"__type__": "uuid",
		"__optional__": true
	},

	"resolution_ts": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"crm_type": {
		"__type__": "string",
		"__options__": ["knk", "ms"],
		"__optional__": true
	},

	"crm_id": {
		"__type__": "string",
		"__maximum__": 36,
		"__optional__": true
	}
}
