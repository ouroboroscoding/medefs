{
	"__mysql__": {
		"auto_primary": "UUID()",
		"create": ["_created","title","type","subject","content"],
		"db": "mems",
		"host": "primary",
		"indexes": {
			"title": {"unique": null}
		},
		"table": "providers_template",
		"charset": "utf8mb4",
		"collate": "utf8mb4_0900_ai_ci"
	},

	"__react__": {
		"create": ["title", "type", "content"],
		"results": ["title", "type", "content"],
		"update": ["title", "type", "content"]
	},

	"__name__": "Providers_Template",

	"_id": {
		"__type__":"uuid",
		"__optional__": true,
		"__react__": {
			"title": "ID"
		}
	},
	"_created": {
		"__type__":"timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP"
		}
	},
	"title": {
		"__type__":"string",
		"__minimum__": 3,
		"__maximum__": 32
	},
	"type": {
		"__type__": "string",
		"__options__": ["note","sms"]
	},
	"subject": {
		"__type__":"string",
		"__minimum__": 3,
		"__maximum__": 128,
		"__optional__": true,
		"__react__": {
			"title": "Subject (email only)"
		}
	},
	"content": {
		"__type__":"string",
		"__minimum__": 3,
		"__maximum__": 65536
	}
}
