{
	"__react__": {
		"create": ["userName", "password", "firstName", "lastName", "email",
					"cellNumber", "notificationPref", "eDFlag", "hormoneFlag",
					"hairLossFlag", "urgentCareFlag", "practiceStates",
					"hrtPracticeStates", "providerScheduleLink", "calendlyEmail",
					"dsClinicId", "dsClinicianId", "claims_max", "claims_timeout",
					"agent"],
		"results": ["userName", "firstName", "lastName"],
		"update": ["userName", "firstName", "lastName", "email",
					"cellNumber", "notificationPref", "eDFlag", "hormoneFlag",
					"hairLossFlag", "urgentCareFlag", "practiceStates",
					"hrtPracticeStates", "providerScheduleLink", "calendlyEmail",
					"dsClinicId", "dsClinicianId", "claims_max", "claims_timeout",
					"agent"]
	},

	"__name__": "Providers_ProviderMemo",

	"_id": {
		"__type__":"uuid",
		"__optional__": true,
		"__react__": {
			"title": "ID"
		}
	},

	"_created": {
		"__type__":"timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP"
		}
	},

	"memo_id": {
		"__type__":"int",
		"__react__": {
			"title": "Memo ID"
		}
	},

	"userName": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "User"
		}
	},

	"password": {
		"__type__": "string",
		"__react__": {
			"title": "Password",
			"regex": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.{8,})"
		}
	},

	"firstName": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "Name"
		}
	},

	"lastName": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "Surname"
		}
	},

	"email": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"title": "E-mail Address"
		}
	},

	"cellNumber": {
		"__type__": "string",
		"__maximum__": 11,
		"__optional__": true,
		"__react__": {
			"title": "Cell Phone"
		}
	},

	"notificationPref": {
		"__type__": "string",
		"__optional__": true,
		"__options__": ["none", "both", "email", "text"],
		"__react__": {
			"options": [["none", "No notifications"], ["both", "Email and SMS"], ["email", "Email"], ["text", "SMS"]],
			"title": "Notification Preference"
		}
	},

	"eDFlag": {
		"__type__": "string",
		"__options__": ["Y", "N"],
		"__react__": {
			"title": "Erectile Dysfunction"
		}
	},

	"hormoneFlag": {
		"__type__": "string",
		"__options__": ["Y", "N"],
		"__react__": {
			"title": "Hormone Replacement Therapy"
		}
	},

	"hairLossFlag": {
		"__type__": "string",
		"__options__": ["Y", "N"],
		"__react__": {
			"title": "Hair Loss"
		}
	},

	"urgentCareFlag": {
		"__type__": "string",
		"__options__": ["Y", "N"],
		"__react__": {
			"title": "Urgent Care"
		}
	},

	"practiceStates": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"extra_space": true,
			"title": "ED Practice States",
			"type": "multiselectcsv"
		}
	},

	"hrtPracticeStates": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"extra_space": true,
			"title": "HRT Practice States",
			"type": "multiselectcsv"
		}
	},

	"providerScheduleLink": {
		"__type__": "string",
		"__optional__": true,
		"__maximum__": 255,
		"__react__": {
			"title": "Calendly Schedule (URL)"
		}
	},

	"calendlyEmail": {
		"__type__": "string",
		"__optional__": true,
		"__maximum__": 63,
		"__react__": {
			"title": "Calendly Email"
		}
	},

	"dsClinicId": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"title": "DoseSpot Clinic ID"
		}
	},

	"dsClinicianId": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"title": "DoseSpot Clinician ID"
		}
	},

	"claims_max": {
		"__type__": "uint",
		"__maximum__": 50,
		"__react__": {
			"default": 20,
			"title": "Maximum self-claims"
		}
	},

	"claims_timeout": {
		"__type__": "uint",
		"__options__": [48, 96, 144],
		"__react__": {
			"default": 48,
			"options": [[48, "2 Days"], [96, "4 Days"], [144, "6 Days"]],
			"title": "Maximum claim hold"
		}
	},

	"agent": {
		"__type__": "int",
		"__optional__": true
	}
}
