		"db": "mems",
		"host": "primary",
		"indexes": {
			"ui_cotm": {"unique": ["crm_type", "crm_id", "crm_order", "type", "medication"]}
		},
		"table": "welldyne_trigger",
		"engine": "InnoDB"
	},

