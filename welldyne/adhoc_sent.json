{
	"__mysql__": {
		"auto_primary": "UUID",
		"create": ["_created", "_updated", "crm_type", "crm_id", "crm_order", "type", "attempts"],
		"db": "mems",
		"host": "primary",
		"indexes": {
			"ui_cot": {"unique": ["crm_type", "crm_id", "crm_order", "type"]}
		},
		"table": "welldyne_adhoc_sent",
		"engine": "InnoDB"
	},

	"__name__": "Welldyne_AdhocSent",

	"_id": {
		"__type__": "uuid",
		"__optional__": true
	},

	"_created": {
		"__type__": "timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP comment 'Date sent'"
		}
	},

	"_updated": {
		"__type__": "timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP"
		}
	},

	"crm_type": {
		"__type__": "string",
		"__options__": ["knk", "mems"],
		"__react__": {
			"title": "CRM"
		}
	},

	"crm_id": {
		"__type__": "string",
		"__maximum__": 36,
		"__react__": {
			"title": "ID"
		}
	},

	"crm_order": {
		"__type__": "string",
		"__maximum__": 36,
		"__react__": {
			"title": "Order"
		}
	},

	"type": {
		"__type__": "string",
		"__options__": ["Update Address", "Cancel Order", "Remove Error", "Extend Eligibility"],
		"__react__": {
			"type": "select"
		}
	},

	"attempts": {
		"__type__": "uint",
		"__optional__": true,
		"__sql__": {
			"opts": "default 1"
		}
	}
}
