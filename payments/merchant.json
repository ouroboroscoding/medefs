{
	"__mysql__": {
		"auto_primary": true,
		"create": ["_created", "name", "gateway", "security_key", "processor_id"],
		"db": "payments",
		"host": "payments",
		"indexes": {
			"ui_name_gateway": {"unique": ["name", "gateway"]},
			"ui_gateway": {"unique": "gateway"}
		},
		"table": "merchant"
	},

	"__name__": "Merchant",

	"_id": {
		"__type__": "uint",
		"__optional__": true
	},
	"_created": {
		"__type__":"timestamp",
		"__optional__": true,
		"__sql_opts__": "default CURRENT_TIMESTAMP"
	},
	"name": {
		"__type__":"string",
		"__maximum__": 31,
		"__sql_type__": "varchar(31)"
	},
	"gateway": {
		"__type__":"string",
		"__options__": ["nmi"],
		"__sql_type__": "varchar(15)"
	},
	"security_key": {
		"__type__": "string",
		"__maximum__": 127,
		"__sql_type__": "varchar(127)"
	},
	"processor_id": {
		"__type__": "string",
		"__maximum__": 127,
		"__sql_type__": "varchar(127)"
	}
}
