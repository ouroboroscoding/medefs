{
	"__mysql__": {
		"auto_primary": false,
		"create": ["_created", "customer", "merchant"],
		"db": "payments",
		"host": "payments",
		"indexes": {
			"ui_customer_merchant": {"unique": ["customer", "merchant"]},
			"merchant": null
		},
		"table": "vault_customer"
	},

	"__name__": "Customer",

	"_id": {
		"__type__": "string",
		"__regex__": "^(?:\\d+|[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12})$",
		"__sql_type__": "char(36)"
	},
	"_created": {
		"__type__":"timestamp",
		"__optional__": true,
		"__sql_opts__": "default CURRENT_TIMESTAMP"
	},
	"customer": {
		"__type__":"uint"
	},
	"merchant": {
		"__type__":"uint"
	}
}
