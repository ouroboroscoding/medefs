{
	"__mysql__": {
		"auto_primary": true,
		"create": ["name", "uri", "type", "state", "provider", "createdAt", "updatedAt"],
		"db": "monolith",
		"host": "monolith",
		"indexes": {
			"name": null
		},
		"primary": "id",
		"table": "calendly_event",
		"engine": "InnoDB",
		"charset": "utf8mb4",
		"collate": "utf8mb4_0900_ai_ci"
	},

	"__react__": {
		"primary": "id",
		"create": ["name", "uri", "type", "state", "provider"],
		"results": ["id", "name", "uri", "type", "state", "provider"],
		"update": ["name", "uri", "type", "state", "provider"]
	},

	"__name__": "CalendlyEvent",

	"id": {
		"__type__": "uint",
		"__optional__": true
	},

	"name": {
		"__type__": "string",
		"__maximum__": 63
	},

	"uri": {
		"__type__": "string",
		"__regex__": "^\/[a-z-]+\/[a-z-]+$",
		"__react__": {
			"title": "Event specific URI: /male-excel-hrt/north-carolina"
		},
		"__sql__": {
			"type": "varchar(63)"
		}
	},

	"type": {
		"__type__": "string",
		"__options__": ["ed", "hrt"],
		"__react__": {
			"options": [["ed", "ED"], ["hrt", "HRT"]],
			"type": "select"
		}
	},

	"state": {
		"__type__": "string",
		"__optional__": true,
		"__regex__": "^[A-Z]{2}$",
		"__react__": {
			"title": "State (optional)",
			"type": "select"
		},
		"__sql__": {
			"type": "char(2)"
		}
	},

	"provider": {
		"__type__": "int",
		"__optional__": true,
		"__react__": {
			"title": "Provider (optional)",
			"type": "select"
		}
	},

	"createdAt": {
		"__type__": "datetime",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP"
		}
	},

	"updatedAt": {
		"__type__": "datetime",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP"
		}
	}
}
