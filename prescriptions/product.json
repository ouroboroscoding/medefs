{
	"__mysql__": {
		"auto_primary": "UUID()",
		"changes": ["user"],
		"create": ["_created", "pharmacy", "key", "display", "ndc", "quantity", "supply", "directions", "unit_id"],
		"db": "mems",
		"host": "primary",
		"indexes": {
			"pharm_key": {"unique": ["pharmacy", "key"]}
		},
		"table": "prescriptions_product",
		"engine": "InnoDB"
	},

	"__react__": {
		"create": ["pharmacy", "key", "display", "ndc", "quantity", "supply", "directions", "unit_id"],
		"results": ["key", "pharmacy", "display", "ndc", "quantity", "supply", "directions", "unit_id"],
		"update": ["key", "display", "ndc", "quantity", "supply", "directions", "unit_id"]
	},

	"__name__": "Prescriptions_Product",

	"_id": {
		"__type__": "uuid",
		"__optional__": true
	},

	"_created": {
		"__type__": "timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP"
		}
	},

	"pharmacy": {
		"__type__": "string",
		"__options__": ["WellDyneRX", "AnazaoHealth"]
	},

	"key": {
		"__type__": "string",
		"__regex__": "^[a-zA-Z0-9_]{1,63}$",
		"__sql__": {
			"type": "varchar(63)"
		}
	},

	"display": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "Medication Display Name"
		}
	},

	"ndc": {
		"__type__": "string",
		"__regex__": "^\\d{10,11}$",
		"__react__": {
			"title": "NDC"
		},
		"__sql__": {
			"type": "varchar(12)"
		}
	},

	"quantity": {
		"__type__": "decimal",
		"__minimum__": "0",
		"__react__": {
			"title": "Medication Quantity"
		}
	},

	"supply": {
		"__type__": "uint",
		"__react__": {
			"title": "Days Supply"
		}
	},

	"directions": {
		"__type__": "string",
		"__maximum__": 255
	},

	"unit_id": {
		"__type__": "uint",
		"__react__": {
			"title": "Dispense Unit ID"
		}
	}
}
