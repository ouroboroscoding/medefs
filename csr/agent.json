{
	"__mysql__": {
		"auto_primary": "UUID()",
		"create": ["_created", "memo_id", "type", "escalate", "label", "claims_max", "oof", "oof_replacement"],
		"db": "mems",
		"host": "primary",
		"indexes": {
			"memo_id": {"unique": null}
		},
		"table": "csr_agent"
	},

	"__name__": "CSR_Agent",

	"_id": {
		"__type__":"uuid",
		"__optional__": true,
		"__react__": {
			"title": "ID"
		}
	},

	"_created": {
		"__type__":"timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP"
		}
	},

	"memo_id": {
		"__type__":"int",
		"__react__": {
			"title": "Memo ID"
		}
	},

	"type": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"extra_space": false,
			"options": [["agent", "Agent"], ["pa", "Provider Assistant"], ["on_hrt", "HRT Onboarding"]],
			"title": "Type",
			"type": "multiselectcsv"
		},
		"__sql__": {
			"opts": "default 'agent'"
		}
	},

	"escalate": {
		"__type__": "bool",
		"__react__": {
			"title": "Allow Escalation to this Agent"
		},
		"__sql__": {
			"opts": "default 0"
		}
	},

	"label": {
		"__type__": "string",
		"__maximum__": 63,
		"__react__": {
			"title": "Escalate / Transfer label"
		},
		"__sql__": {
			"type": "varchar(63)",
			"opts": "default ''"
		}
	},

	"claims_max": {
		"__type__": "uint",
		"__maximum__": 50,
		"__sql__": {
			"type": "tinyint unsigned"
		},
		"__react__": {
			"title": "Maximum self-claims"
		}
	},

	"oof": {
		"__type__": "bool",
		"__sql__": {
			"opts": "default 0"
		}
	},

	"oof_replacement": {
		"__type__": "int",
		"__sql__": {
			"opts": "default 0"
		}
	}
}
