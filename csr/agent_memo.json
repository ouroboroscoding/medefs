{
	"__react__": {
		"create": ["userName", "password", "firstName", "lastName", "email", "dsClinicId", "dsClinicianId", "type", "escalate", "label", "claims_max"],
		"results": ["userName", "firstName", "lastName", "type"],
		"update": ["userName", "firstName", "lastName", "email", "dsClinicId", "dsClinicianId", "type", "escalate", "label", "claims_max"]
	},

	"__name__": "CSR_AgentMemo",

	"_id": {
		"__type__":"uuid",
		"__optional__": true,
		"__react__": {
			"title": "ID"
		}
	},

	"_created": {
		"__type__":"timestamp",
		"__optional__": true,
		"__sql__": {
			"opts": "default CURRENT_TIMESTAMP"
		}
	},

	"memo_id": {
		"__type__":"int",
		"__react__": {
			"title": "Memo ID"
		}
	},

	"userName": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "User"
		}
	},

	"password": {
		"__type__": "string",
		"__react__": {
			"title": "Password",
			"regex": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.{8,})"
		}
	},

	"firstName": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "Name"
		}
	},

	"lastName": {
		"__type__": "string",
		"__maximum__": 255,
		"__react__": {
			"title": "Surname"
		}
	},

	"email": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"title": "E-mail Address"
		}
	},

	"dsClinicId": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"title": "DoseSpot Clinic ID"
		}
	},

	"dsClinicianId": {
		"__type__": "string",
		"__maximum__": 255,
		"__optional__": true,
		"__react__": {
			"title": "DoseSpot Clinician ID"
		}
	},

	"type": {
		"__type__": "string",
		"__optional__": true,
		"__react__": {
			"extra_space": false,
			"options": [["agent", "Agent"], ["pa", "Provider Assistant"], ["on_hrt", "HRT Onboarding"]],
			"title": "Type",
			"type": "multiselectcsv"
		},
		"__sql__": {
			"opts": "default 'agent'"
		}
	},

	"escalate": {
		"__type__": "bool",
		"__react__": {
			"title": "Allow Escalation to this Agent"
		},
		"__sql__": {
			"opts": "default 0"
		}
	},

	"label": {
		"__type__": "string",
		"__maximum__": 63,
		"__react__": {
			"title": "Escalate / Transfer label"
		}
	},

	"claims_max": {
		"__type__": "uint",
		"__maximum__": 50,
		"__react__": {
			"default": 20,
			"title": "Maximum self-claims"
		}
	}
}
